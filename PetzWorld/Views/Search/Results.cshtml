@if (Model.Count == 0)
    {
     <p>No dogs yet!</p>
    }

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Breed</th>
        <th>Age</th>
    </tr>

    @foreach (Dog Dog in Model)
    {
        <tr>
            <td>@Dog.Id</td>
            <td>@Dog.Name</td>
            <td>@Dog.Breed</td>
            <td>@Dog.Age</td>
        </tr>
    }
</table>